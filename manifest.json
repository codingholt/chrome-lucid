{
  "name": "Lucid",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "Replace your new tab page with a greeting and simple notepad.",
  "homepage_url": "https://github.com/daneden/chrome-lucid",
  "icons": {
    "32" : "icons/tab32.png",
    "64" : "icons/tab128.png",
    "128" : "icons/tab128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": ["storage"]
}
